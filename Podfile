  # For example, to use Firebase Authentication and Cloud Firestore
  pod 'Firebase/Auth'
  pod 'Firebase/Firestore'
  pod 'Firebase/Database'

  # Pods for Lyncv2
  

  target 'Lyncv2Tests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Lyncv2UITests' do
    # Pods for testing
  end

end
